{
    "plugins": ["prettier-plugin-tailwindcss"],
    "tabWidth": 4,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "trailingComma": "all",
    "quoteProps": "consistent",
    "semi": true,
    "useTabs": false,
    "endOfLine": "lf",
    "singleQuote": true,
    "jsxSingleQuote": false,
    "printWidth": 120
}
